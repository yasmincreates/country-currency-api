{
  "name": "country-currency-api",
  "version": "1.0.0",
  "description": "RESTful API for country data with currency exchange rates and GDP estimation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "api",
    "countries",
    "currency",
    "exchange-rates",
    "gdp"
  ],
  "author": "Yasmin Abdulrahman",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "canvas": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
